---
import config from "@/config/config.json";
import { Image } from "astro:assets";
const { src, showText } = Astro.props;
const {
  logo_text,
  logo_width,
  logo_height,
  title,
}: {
  logo: string;
  logo_text: string;
  logo_width: string;
  logo_height: string;
  title: string;
} = config.site;

const text = logo_text;
---

<a href="/" class="navbar-brand flex flex-row items-center">
      <Image
        class="h-20 w-auto"
        src='/images/logo-trans.png'
        alt='carpet refresh borders'
        width={Number(3009)}
        height={Number(1017)}
      />
</a>
